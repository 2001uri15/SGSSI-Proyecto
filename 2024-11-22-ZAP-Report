            &lt;input type=&quot;hidden&quot; name=&quot;csrf_token&quot; value=&quot;1ea54e565f78ed88e8e25fcf884a01f7718e15a7c7c20200f1129fd6b3632f3d&quot;&gt;
            &lt;button type=&quot;submit&quot; class=&quot;login-button&quot; id=&quot;login_submit&quot;&gt;Iniciar Sesión&lt;/button&gt;
